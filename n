#Data Types

#string

print("Hello"[0])
print("Hello"[1])
print("Hello"[2])
print("Hello"[3])
print("Hello"[4])

#Interger
print(123 + 456)
print(1000 * 0.02)
print(50 * 1250)
#Float
print(34563.79)
print(7.899854)

#Boolean
True
False

num_char = len(input("What is your name?"))
new_num_char = str(num_char)

print("Your name has " + new_num_char + " characters.")

a = float(123)
print(type(a))

print(70 + float("100.5"))